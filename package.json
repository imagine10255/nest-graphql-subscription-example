{
  "name": "nest-graphql-subscription-example",
  "description": "nest-graphql-subscription fail example",
  "version": "1.0.0",
  "author": "imagine10255@gmail.com",
  "license": "MIT",
  "private": true,
  "engines": {
    "npm": ">=6",
    "node": ">=18.13.0"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "script-cli": "ts-node --project _deploy/config/tsconfig.json -r tsconfig-paths/register",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "dev": "yarn prebuild && nest start --watch",
    "dev:db": "docker-compose up",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "resolutions": {
    "redis": "3.0.2"
  },
  "dependencies": {
    "@nestjs/apollo": "^10.1.7",
    "@nestjs/cli": "9.1.5",
    "@nestjs/common": "9.1.6",
    "@nestjs/config": "2.2.0",
    "@nestjs/core": "9.1.6",
    "@nestjs/graphql": "^10.1.7",
    "@nestjs/mapped-types": "^1.2.0",
    "@nestjs/platform-express": "9.1.6",
    "@nestjs/testing": "9.1.6",
    "@types/express": "4.17.14",
    "@types/graphql": "^14.5.0",
    "@types/jest": "29.2.2",
    "@types/node": "18.11.9",
    "@types/supertest": "2.0.12",
    "apollo-server-express": "^3.11.1",
    "dotenv": "10.0.0",
    "graphql": "^16.6.0",
    "node-os-utils": "1.3.2",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.4.0",
    "type-graphql": "^1.1.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.29.2",
    "@typescript-eslint/parser": "4.29.2",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.4.1",
    "jest": "27.3.0",
    "prettier": "^2.8.6",
    "source-map-support": "0.5.20",
    "supertest": "6.1.6",
    "ts-jest": "27.0.7",
    "ts-loader": "9.3.1",
    "ts-node": "10.8.1",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      ".+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
